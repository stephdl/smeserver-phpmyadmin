#!/usr/bin/perl -w

use esmith::Build::CreateLinks qw(:all);

for my $event (qw(
    nethserver-phpmyadmin-conf
    ))
{
    templates2events("/etc/phpMyAdmin/config.inc.php", $event)
    templates2events("/etc/httpd/conf.d/phpMyAdmin.conf", $event)
    safe_symlink("restart", "root/etc/e-smith/events/$event/services2adjust/httpd");

}
